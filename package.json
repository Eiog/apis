{
  "name": "node-server-starter",
  "type": "commonjs",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "author": "Eiog",
  "license": "MIT",
  "homepage": "https://github.com/Eiog/node-server-starter",
  "main": "src/app.ts",
  "scripts": {
    "dev:vercel": "tsx scripts/generate-router.ts && set mode=dev&& tsx watch scripts/vercel.dev.ts",
    "build": "rimraf dist && tsc",
    "tsup": "tsup --watch",
    "tsup:build": "tsup",
    "start": "node dist/node.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "update:deps": "taze -w && pnpm i",
    "type:check": "tsc --noEmit",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "doydl": "^0.1.2",
    "express": "^4.18.2",
    "h3": "^1.10.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^2.0.6",
    "https-proxy-agent": "^7.0.2",
    "node-json-db": "^2.3.0",
    "object-path": "^0.11.8",
    "qiniu": "^7.11.0",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@eiog/eslint-config-ts": "^0.0.2",
    "@eiog/ts-config": "^0.0.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/object-path": "^0.11.4",
    "chalk": "^5.3.0",
    "commitlint": "^18.5.0",
    "consola": "^3.2.3",
    "cz-git": "^1.8.0",
    "czg": "^1.8.0",
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.0",
    "listhen": "^1.5.6",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vercel": "^33.2.0",
    "vitest": "^1.2.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
}
